{
  "rewrites": [
    { "source": "/page2", "destination": "/page2.html" },
    { "source": "/page2.html", "destination": "/page2.html" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "trailingSlash": false,
  "cleanUrls": false,
  "outputDirectory": "dist"
}